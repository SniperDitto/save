
public class ChgType {

	public static void main(String[] args) {
		
		//형 변환
		
		//1. 묵시적 형변환
		//메모리가 작은 데이터(4byte) -> 큰 데이터(8byte)
		int num1 = 123;
		long num2 = num1;//int->long
		
		float f = 123.456f;
		double d = f;//float->double
		
		//2. 명시적 형변환 - 메모리 공간을 생각해야 한다
		//메모리가 큰 데이터(8byte) -> 작은 데이터(4byte)
		long num3 = 1234;
		int num4 = (int)num3;//long->int
		
		
		
		
		
		
	}

}
