
public class Customer {
	
	
	//객체 - 속성 & 행위
	
	//속성 : property, attribute
	//행위 : method
	
	//Customer 클래스의 속성들
	public String cID;
	public String cName;
	public int cAge;
	public String cGender;//char 추천
	
	
	//Customer 클래스의 행위들
	public void buyProduct(String pName, int pPrice) {
		
		//~세 홍길동이 ~를 ~에 구매했다
		String strResult = cAge+"세 "+cName+"이 "+pName+"을 "+pPrice+"에 구매했다.";
		
	}
	
	
}
