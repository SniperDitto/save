package pkg.names;

public class DataHandle {
	private String name;
	private String age;
	
	public void setName(String name) {
		this.name = name;
	}

	public void setAge(String age) {
		this.age = age;
	}

	//data1
	public String getName() {
		return this.name;
	}
	
	//data2
	public String getAge() {
		return this.age;
	}
}
