package pkg.names.exec;

import pkg.names.DAO.DataDAO;

public class Exec {

	public static void main(String[] args) {
		
		DataDAO dataDAO = new DataDAO();
		
		//완짱은 52살이다
		String name = dataDAO.getName();
		String age = dataDAO.getAge();
		System.out.printf("%s은 %s살이다.", name, age);
		
		
	}

}
