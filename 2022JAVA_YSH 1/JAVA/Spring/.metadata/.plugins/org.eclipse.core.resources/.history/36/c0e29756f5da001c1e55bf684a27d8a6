package pkg.names;

public class DataHandle {
	private String name;
	private String age;
	
	public DataHandle() {}
	public DataHandle(String name, String age) {
		//db에서 가져왔다고 가정
		this.name = name;
		this.age = age;
	}
	
	public String getName() {
		//db에서 가져왔다고 가정
		return this.name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	public String getAge() {
		//db에서 가져왔다고 가정
		return this.age;
	}
	public void setAge(String age) {
		this.age = age;
	}
}
