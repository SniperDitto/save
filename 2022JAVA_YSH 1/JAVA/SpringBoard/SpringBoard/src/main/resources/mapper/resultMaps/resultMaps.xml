<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="pkg.admin.maps">

<resultMap id="authMap" type="pkg.admin.DTO.AuthDTO">
	<result property="authID" column="AUTHID"></result>
	<result property="authName" column="AUTHNAME"></result>
</resultMap>

<resultMap id="authMemberMap" type="pkg.admin.DTO.AuthMemberDTO">
	<result property="mID" column="MID"></result>
	<result property="mName" column="MNAME"></result>
	<result property="authID" column="AUTHID"></result>
	<result property="authName" column="AUTHNAME"></result>
</resultMap>

<resultMap id="programMap" type="pkg.admin.DTO.ProgramDTO">
	<result property="pID" column="PID"></result>
	<result property="pName" column="PNAME"></result>
	<result property="menuID" column="MENUID"></result>
	<result property="pFileName" column="PFILENAME"></result>
	<result property="fileUrl" column="FILEURL"></result>
</resultMap>

<resultMap id="SMenuMap" type="pkg.admin.DTO.MenuDTO">
	<result property="menuID" column="MENUID"></result>
	<result property="menuName" column="MENUNAME"></result>
	<result property="menuSeq" column="MENUSEQ"></result>
	<result property="menuLvl" column="MENULVL"></result>
	<result property="parentID" column="PARENTID"></result>
</resultMap>

</mapper>