package pkg.test;

public class Sorting {

	public static void main(String[] args) {
		
		int[] arr = {10, 11, 2, 5, 3, 3, 24, 15, 6, 9};
		
		
		
	}
	
	public int[] SortArr(int[] arr) {
		//int[] result = new int[arr.length];
		
		for(int i=0;i<arr.length;i++) {
			for(int j=arr.length;j>0;j--) {
				if(arr[j]<arr[j-1]) {
					int temp = arr[j];
					arr[j] = arr[j-1];
					arr[j-1] = temp;
				}
			}
			
		}
		
		return arr;
	}

}
